.card1,.card2,.card3,.card4    
  width 500px
  height 280px
  position absolute
  top 100px
  overflow hidden
  background #A02017
  background-image url('../../dist/images/about-card/background-acland-pavement-960x600-20pc-opacity.png')
  
.card1
    position absolute
    top 80px
      

.card2
    position absolute
    top 80px
    

.card3
    position absolute
    top 400px
    

.card4
    position absolute
    top 400px
    
    
.card1:before,.card2:before,.card3:before,.card4:before
  content: "";
  width: 200%;
  height: 100%;
  position: absolute;
  background: #263A45;
  transform: rotate(-30deg);
  top: -200px;
  left: -140px;
  box-shadow: 0 0 10px 0 #000;
  z-index: 2;
  background-image: url('../../dist/images/about-card/2634310431_ccae90c2b1_o.png');
  background-size: 100%;


.card1:after,.card2:after,.card3:after,.card4:after
  content: "";
  width: 100%;
  height: 100%;
  position: absolute;
  background: #263A45;
  transform: rotate(48deg);
  bottom: -190px;
  left: -60px;
  box-shadow: 0 0 10px 0 #000;
  z-index: 1;
  background-image: url('../../dist/images/about-card/2634310431_ccae90c2b1_o.png');
  background-size: 150%;


