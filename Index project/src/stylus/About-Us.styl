@import 'green-theme'
@import 'blue-theme'
@import 'yellow-theme'
@import 'defalut-theme'


body
    overflow hidden
    
h1,h2,h3,h4,.lead
    font-family  'Droid Sans', sans-serif;    
/**********  Start Overriding Bootstrap *********/

/* Main Navbar */
.navbar
    min-height 70px
    font-size 18px
/* Active linke*/


     
/*Normal Links */    
.navbar-inverse .navbar-nav > li > a
    color #BBB
    padding-bottom 25px
    padding-top 25px
    
/* brand name*/
.navbar-inverse .navbar-brand
    color #BBB
    line-height 40px
    font-size 30px
    
/*Dropdown menu */
.dropdown-menu
    background-color #333
   
    min-width 200px
    
.navbar-right .dropdown-menu
    left 0
    right auto
    
.dropdown-menu > li > a
    color #fff
    padding 8px 20px
    -webkit-transition : padding .5s
    -o-transition : padding .5s 
    -moz-transition : padding .5s
    transition : padding .5s
    
.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus
  
    color #fff
    padding-left 30px


/*Navbar Toggel */


.navbar-toggle
    margin-top 18px

/*Start breadcrumb*/
.breadcrumb-holder
    background-color #f5f5f5
.breadcrumb
    margin-top 70px
    margin-bottom 0px
    padding 10px 0
.about-us
    padding 70px 0
.about-us h1
    font-family 'Raleway', sans-serif
    letter-spacing -2px
    font-size 62px
.about-us hr
    width 70%
    
.about-us img
    height 600px
    
.about-features
    padding 0 0 70px 0
    
.about-features h3
    font-family 'Raleway', sans-serif
    
.about-features p
    margin-bottom 20px

.about-mem 
    background-color #eee
    padding-top 50px 
.about-mem p
    line-height 1.8
.about-mem h2    
    font-family 'Raleway', sans-serif
.about-mem .btn-primary
    background-color #333
    border-color #333
    margin-bottom 30px
.about-mem img
    width 300px
    height 300px
/*End breadcrumb*/
/*Start Footer*/
.footer
    background-color #555
    color #AAA
    padding-top 40px
.footer a
    color #AAA
    
.footer a:hover
    color #fff
    
.footer h3
   
    letter-spacing 2px
    margin 30px

.footer .three-columens
    overflow hidden
    
.footer .three-columns li
    list-style none
    width 33.3333%
    float left
    padding 5px 0
.footer .social-list
    margin 20px 0 10px
.footer .social-list li
    float left
    margin 5px
    opacity .7
    -webkit-transition all .4s ease-in-out
    -o-transition all .4s ease-in-out
    -moz-transition all .4s ease-in-out
    transition all .4s ease-in-out
.footer .social-list li:hover
    opacity 1
.img-thumbnail
    background-color rgba(0,0,0,.6)
    border 1px solid #666
    margin-bottom 5px
    
.footer .copyright
    background-color #333
    color #AAA
    padding 15px 0
    margin-top 20px
    font-size 20px
    text-align center
    
.footer .copyright span
    font-weight bold
  
/*End Footer*/

/* Start tool box section*/
.option-box
    z-index 9999
    position fixed
    top 100px
    left 0px
    
.option-box .color-option
    float left
    padding 10px
    background-color #333
    color #fff
    width 200px
    
    display none
    
    
.color-option ul li
    cursor pointer
    width 20px
    height 20px
    display inline-block
    margin-right 3px
    
 .option-box .gear-check
    padding 5px
    
    color #fff
    cursor pointer 
/* End tool box section*/

/*Start loading section*/
.loading-overlay
    background-color #bd8f5e
    color #fff
    position fixed
    left 0
    top 0
    width 100%
    height 100%
    z-index 99999
.sk-folding-cube {
  margin: 300px auto;
  width: 60px;
  height: 60px;
  position: relative;
  -webkit-transform: rotateZ(45deg);
          transform: rotateZ(45deg);
}

.sk-folding-cube .sk-cube {
  float: left;
  width: 50%;
  height: 50%;
  position: relative;
  -webkit-transform: scale(1.1);
      -ms-transform: scale(1.1);
          transform: scale(1.1); 
}
.sk-folding-cube .sk-cube:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  -webkit-animation: sk-foldCubeAngle 2.4s infinite linear both;
          animation: sk-foldCubeAngle 2.4s infinite linear both;
  -webkit-transform-origin: 100% 100%;
      -ms-transform-origin: 100% 100%;
          transform-origin: 100% 100%;
}
.sk-folding-cube .sk-cube2 {
  -webkit-transform: scale(1.1) rotateZ(90deg);
          transform: scale(1.1) rotateZ(90deg);
}
.sk-folding-cube .sk-cube3 {
  -webkit-transform: scale(1.1) rotateZ(180deg);
          transform: scale(1.1) rotateZ(180deg);
}
.sk-folding-cube .sk-cube4 {
  -webkit-transform: scale(1.1) rotateZ(270deg);
          transform: scale(1.1) rotateZ(270deg);
}
.sk-folding-cube .sk-cube2:before {
  -webkit-animation-delay: 0.3s;
          animation-delay: 0.3s;
}
.sk-folding-cube .sk-cube3:before {
  -webkit-animation-delay: 0.6s;
          animation-delay: 0.6s; 
}
.sk-folding-cube .sk-cube4:before {
  -webkit-animation-delay: 0.9s;
          animation-delay: 0.9s;
}
@-webkit-keyframes sk-foldCubeAngle {
  0%, 10% {
    -webkit-transform: perspective(140px) rotateX(-180deg);
            transform: perspective(140px) rotateX(-180deg);
    opacity: 0; 
  } 25%, 75% {
    -webkit-transform: perspective(140px) rotateX(0deg);
            transform: perspective(140px) rotateX(0deg);
    opacity: 1; 
  } 90%, 100% {
    -webkit-transform: perspective(140px) rotateY(180deg);
            transform: perspective(140px) rotateY(180deg);
    opacity: 0; 
  } 
}

@keyframes sk-foldCubeAngle {
  0%, 10% {
    -webkit-transform: perspective(140px) rotateX(-180deg);
            transform: perspective(140px) rotateX(-180deg);
    opacity: 0; 
  } 25%, 75% {
    -webkit-transform: perspective(140px) rotateX(0deg);
            transform: perspective(140px) rotateX(0deg);
    opacity: 1; 
  } 90%, 100% {
    -webkit-transform: perspective(140px) rotateY(180deg);
            transform: perspective(140px) rotateY(180deg);
    opacity: 0; 
  }
}
/*End loading section*/
/*Start scrollup */
.scroll-top
    width 50px
    height 40px
    text-align center
    color #fff
    background-color #333
    position fixed
    right 12px
    bottom  10px
    z-index 9999
    display none
    cursor pointer
    
.scroll-top:hover
    background-color #888
/*End scrollup */